ef(null);\r\n  const handleSearch = (e) => {\r\n    e.preventDefault();\r\n    if (searchData) {\r\n      let item = categories[activeType].filter((i) =>\r\n        i.name.toLowerCase().includes(searchData.toLowerCase())\r\n      );\r\n      setDataItems([...item]);\r\n    } else {\r\n      setDataItems([...categories[activeType]]);\r\n      searchRef.current.focus();\r\n    }\r\n  };\r\n\r\n  const handelLoader = (id, val) => {\r\n    setLoader({ ...loader, [id]: val });\r\n  };\r\n\r\n  const getData = async () => {\r\n    handelLoader(\"page\", true);\r\n    let cats = await getCategories();\r\n    setDataItems([...cats[activeType]]);\r\n    handelLoader(\"page\", false);\r\n  };\r\n\r\n  useEffect(() => {\r\n    getData();\r\n  }, [activeType]);\r\n\r\n  const handleChoose = (v) => {\r\n    setActiveType(v);\r\n    setSearchData(\"\");\r\n  };\r\n\r\n  const handleDelete = async (id) => {\r\n    handelLoader(\"delete\", true);\r\n    try {\r\n      const res = await Axios({\r\n        baseURL: API,\r\n        url: \"/api/categories/delete\",\r\n        method: \"DELETE\",\r\n        params: { id },\r\n      });\r\n      await getData(res.data.data);\r\n    } catch (error) {\r\n      console.log(error.response);\r\n      alert(\"فشل حذف الفئه\");\r\n    }\r\n    handelLoader(\"delete\", false);\r\n  };\r\n\r\n  const handleEdit = async (info) => {\r\n    handelLoader(\"edit\", true);\r\n    try {\r\n      const res = await Axios({\r\n        baseURL: API,\r\n        url: \"/api/categories/edit\",\r\n        method: \"POST\",\r\n        data: info,\r\n      });\r\n      await getData(res.data.data);\r\n    } catch (error) {\r\n      console.log(error.response ?? error);\r\n      alert(\"فشل تعديل الفئه\");\r\n    }\r\n    handelLoader(\"edit\", false);\r\n  };\r\n\r\n  console.log(dataItems);\r\n\r\n  const columns = [\r\n    {\r\n      name: \"\",\r\n      selector: (row) => (\r\n        <img src={`${API}/${row.image}`} className=\"w-16 py-3\" />\r\n      ),\r\n    },\r\n    { name: \"الاسم\", selector: (row) => row.name, sortable: true },\r\n    {\r\n      name: \"نوع الفئة\",\r\n      selector: (row) => getTypeName(row.type),\r\n      sortable: true,\r\n    },\r\n  ];\r\n\r\n  return (\r\n    <MainPageContainer className=\"\" title=\"الفئات\" addLink={\"/categories/add\"}>\r\n      {loader.page ? (\r\n        <div className=\"flex justify-center items-center\">\r\n          <div className=\"material-icons text-6xl text-primary animate-spin\">\r\n            track_changes\r\n          </div>\r\n        </div>\r\n      ) : (\r\n        <>\r\n          <div className=\"w-full p-3 bg-white rounded-lg\">\r\n            <select\r\n              value={activeType}\r\n              className=\"w-full\"\r\n              onChange={(e) => handleChoose(e.target.value)}\r\n            >\r\n              <option value=\"all\">جميع الفئات</option>\r\n              <option value=\"service\">فئات الخدمات</option>\r\n              <option value=\"product\">تسوق اونلاين</option>\r\n            </select>\r\n            <form\r\n              onSubmit={handleSearch}\r\n              className=\"flex space-x-3 space-x-reverse py-3 justify-between\"\r\n            >\r\n              <input\r\n                value={searchData}\r\n                type=\"search\"\r\n                ref={searchRef}\r\n                onChange={(e) => setSearchData(e.target.value)}\r\n                placeholder=\"بحث...\"\r\n                className=\"flex-grow\"\r\n              />\r\n              <button className=\"material-icons\" type=\"submit\">\r\n                search\r\n              </button>\r\n            </form>\r\n          </div>\r\n          {dataItems.length >= 1 ? (\r\n            <>\r\n              <div className=\"p-2 rounded-md bg-white\">\r\n                <DataTable columns={columns} data={dataItems} />\r\n              </div>\r\n            </>\r\n          ) : (\r\n            <EmptyCard />\r\n          )}\r\n        </>\r\n      )}\r\n    </MainPageContainer>\r\n  );\r\n};\r\n\r\nexport default ShopOnline;\r\n\r\nexport const getServerSideProps = (ctx) => {\r\n  const { user } = NextCookies(ctx);\r\n  if (!user && ctx.res) {\r\n    ctx.res.writeHead(301, { location: \"/login\" });\r\n    ctx.res.end();\r\n  }\r\n  return { props: {} };\r\n};\r\n"]},"metadata":{},"sourceType":"module"}