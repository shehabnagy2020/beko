r_Products.map((product) => {\n          if (product.Product == null) hasNullableProducts = true;\n        });\n        if (hasNullableProducts == false && order.User != null) {\n          if (!oldVals.includes(order.id)) {\n            order.new = true;\n            // oldVals.push(order.id);\n          }\n          newValues.push(order);\n        }\n      });\n      setDataItems([...newValues]);\n      // localStorage.setItem(\"ordersIDs\", JSON.stringify(oldVals));\n      handelLoader(\"page\", false);\n    } catch (error) {}\n  };\n  useEffect(() => {\n    getData();\n  }, []);\n\n  const handleDelete = async (id) => {\n    handelLoader(\"delete\", true);\n    try {\n      const res = await Axios({\n        baseURL: API,\n        url: `/api/order?id=${id}`,\n        method: \"DELETE\",\n      });\n      getData();\n    } catch (error) {\n      alert(\"فشل حذف \");\n    }\n    handelLoader(\"delete\", false);\n  };\n\n  const handleEdit = async (info) => {\n    handelLoader(\"edit\", true);\n    try {\n      const res = await Axios({\n        baseURL: API,\n        url: \"/api/orders/edit\",\n        method: \"POST\",\n        data: info,\n      });\n      getData();\n    } catch (error) {\n      console.log(error.response);\n      alert(\"فشل تعديل \");\n    }\n    handelLoader(\"edit\", false);\n  };\n\n  const columns = [\n    {\n      name: \"الاسم\",\n      selector: (row) => (\n        <div className=\"gap-x-3 flex items-center\">\n          {row.new && (\n            <div className=\"animate-pulse w-2 h-2 bg-green-400 rounded-full\"></div>\n          )}\n          <span>{row.User.full_name}</span>\n        </div>\n      ),\n      sortable: true,\n    },\n    {\n      name: \"المجموع الكلي\",\n      selector: (row) => row.total_price,\n      sortable: true,\n    },\n    {\n      name: \"العنوان\",\n      selector: (row) => row.User.address,\n      sortable: true,\n    },\n    {\n      name: \"تاريخ الارسال\",\n      selector: (row) => Moment(row.created_at).format(\"yyyy/MM/DD\"),\n      sortable: true,\n    },\n    // {\n    //   name: \"\",\n    //   selector: (row) => (\n    //     <div className=\"gap-x-3 flex py-3\">\n    //       <Link href={`/orders/edit/${row.id}`}>\n    //         <a className=\"bg-primary hover:bg-secondary p-2 rounded-lg text-white\">\n    //           <span className=\"material-icons text-lg\">edit</span>\n    //         </a>\n    //       </Link>\n    //     </div>\n    //   ),\n    //   sortable: true,\n    // },\n    {\n      name: \"\",\n      selector: (row) => (\n        <div className=\"gap-x-3 flex \">\n          <Link href={`/orders/edit/${row.id}`}>\n            <a className=\"bg-primary hover:bg-secondary p-2 rounded-lg text-white\">\n              <span className=\"material-icons text-lg\">edit</span>\n            </a>\n          </Link>\n          <ButtonLoader loader={loader.delete === row.id} color=\"red-500\">\n            <button\n              className=\"bg-red-500 hover:bg-red-400 p-2 rounded-lg text-white\"\n              onClick={(_) => handleDelete(row.id)}\n            >\n              <span className=\"material-icons text-lg\">delete</span>\n            </button>\n          </ButtonLoader>\n        </div>\n      ),\n      sortable: true,\n    },\n  ];\n  return (\n    <MainPageContainer className=\"\" title=\"الطلبات\">\n      {loader.page ? (\n        <div className=\"flex justify-center items-center\">\n          <div className=\"material-icons text-6xl text-primary animate-spin\">\n            track_changes\n          </div>\n        </div>\n      ) : (\n        <>\n          {dataItems.length >= 1 ? (\n            <div className=\"p-2 rounded-md bg-white\">\n              <DataTable\n                columns={columns}\n                data={dataItems}\n                pagination\n                paginationComponentOptions={{\n                  rowsPerPageText: \"النتائج في الصفحة\",\n                }}\n              />\n            </div>\n          ) : (\n            <EmptyCard />\n          )}\n        </>\n      )}\n    </MainPageContainer>\n  );\n};\n\nexport default Orders;\n\nexport const getServerSideProps = (ctx) => {\n  const { user } = NextCookies(ctx);\n  if (!user && ctx.res) {\n    ctx.res.writeHead(301, { location: \"/login\" });\n    ctx.res.end();\n  }\n  return { props: {} };\n};\n"]},"metadata":{},"sourceType":"module"}