lter((i) => i.type === activeType);\r\n        setOriginalData([...values]);\r\n        setDataItems([...values]);\r\n      }\r\n      handelLoader(\"page\", false);\r\n    } catch (error) {}\r\n  };\r\n\r\n  useEffect(() => {\r\n    getData();\r\n  }, [activeType]);\r\n\r\n  const handleChoose = (v) => {\r\n    setActiveType(v);\r\n    setActiveItem(0);\r\n  };\r\n\r\n  const handleDelete = async (id) => {\r\n    handelLoader(\"delete\", true);\r\n    try {\r\n      const res = await Axios({\r\n        baseURL: API,\r\n        url: \"/offer/delete\",\r\n        method: \"DELETE\",\r\n        params: { id },\r\n      });\r\n      getData();\r\n    } catch (error) {\r\n      alert(\"فشل حذف \");\r\n    }\r\n    handelLoader(\"delete\", false);\r\n  };\r\n  const handleEdit = async (info) => {\r\n    handelLoader(\"edit\", true);\r\n    try {\r\n      const res = await Axios({\r\n        baseURL: API,\r\n        url: \"/offer/edit\",\r\n        method: \"POST\",\r\n        data: info,\r\n      });\r\n      getData();\r\n    } catch (error) {\r\n      alert(\"فشل تعديل \");\r\n    }\r\n    handelLoader(\"edit\", false);\r\n  };\r\n  return (\r\n    <MainPageContainer className=\"\" title=\"العروض\" addLink={\"/offers/add\"}>\r\n      {loader.page ? (\r\n        <div className=\"flex justify-center items-center\">\r\n          <div className=\"material-icons text-6xl text-primary animate-spin\">\r\n            track_changes\r\n          </div>\r\n        </div>\r\n      ) : dataItems.length >= 1 ? (\r\n        <>\r\n          <div className=\"w-full p-3 bg-white rounded-lg\">\r\n            <select\r\n              value={activeType}\r\n              className=\"w-full\"\r\n              onChange={(e) => handleChoose(e.target.value)}\r\n            >\r\n              <option value=\"all\">جميع الفئات</option>\r\n              <option value=\"service\">فئات الخدمات</option>\r\n              <option value=\"product\">تسوق الان</option>\r\n            </select>\r\n            <form\r\n              onSubmit={handleSearch}\r\n              className=\"flex space-x-3 space-x-reverse py-3 justify-between\"\r\n            >\r\n              <input\r\n                value={searchData}\r\n                type=\"search\"\r\n                ref={searchRef}\r\n                onChange={(e) => setSearchData(e.target.value)}\r\n                placeholder=\"بحث...\"\r\n                className=\"flex-grow\"\r\n              />\r\n              <button className=\"material-icons\" type=\"submit\">\r\n                search\r\n              </button>\r\n            </form>\r\n          </div>\r\n          <SwitchTransition>\r\n            <CSSTransition\r\n              key={activeItem.id}\r\n              classNames=\"fade\"\r\n              appear={true}\r\n              timeout={300}\r\n              unmountOnExit={true}\r\n              exit={true}\r\n            >\r\n              <>\r\n                <OffersCard\r\n                  item={activeItem}\r\n                  handleDelete={handleDelete}\r\n                  handleEdit={handleEdit}\r\n                  loader={loader}\r\n                />\r\n              </>\r\n            </CSSTransition>\r\n          </SwitchTransition>\r\n          <Paganition\r\n            activeItem={activeItem}\r\n            setActiveItem={setActiveItem}\r\n            dataItems={dataItems}\r\n          />\r\n        </>\r\n      ) : (\r\n        <EmptyCard />\r\n      )}\r\n    </MainPageContainer>\r\n  );\r\n};\r\n\r\nexport default Offers;\r\n\r\nexport const getServerSideProps = (ctx) => {\r\n  const { user } = NextCookies(ctx);\r\n  if (!user && ctx.res) {\r\n    ctx.res.writeHead(301, { location: \"/login\" });\r\n    ctx.res.end();\r\n  }\r\n  return { props: {} };\r\n};\r\n"]},"metadata":{},"sourceType":"module"}