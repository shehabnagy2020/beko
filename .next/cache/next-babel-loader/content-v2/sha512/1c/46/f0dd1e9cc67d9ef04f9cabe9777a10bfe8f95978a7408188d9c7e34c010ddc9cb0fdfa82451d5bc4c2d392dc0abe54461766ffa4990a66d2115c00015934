 else {\r\n        values = values.filter((i) => i.type === activeType);\r\n        setOriginalData([...values]);\r\n        setDataItems([...values]);\r\n      }\r\n      handelLoader(\"page\", false);\r\n    } catch (error) {}\r\n  };\r\n\r\n  useEffect(() => {\r\n    getData();\r\n  }, [activeType]);\r\n\r\n  useEffect(() => {\r\n    if (dataItems.length >= 1) setActiveItem(dataItems[0]);\r\n  }, [dataItems]);\r\n\r\n  const handleChoose = (v) => {\r\n    setActiveType(v);\r\n  };\r\n\r\n  const handleDelete = async (id) => {\r\n    handelLoader(\"delete\", true);\r\n    try {\r\n      const res = await Axios({\r\n        baseURL: API,\r\n        url: \"/api/offer/delete\",\r\n        method: \"DELETE\",\r\n        params: { id },\r\n      });\r\n      await getData([...res.data.data]);\r\n    } catch (error) {\r\n      console.log(error.response);\r\n      alert(\"فشل حذف \");\r\n    }\r\n    handelLoader(\"delete\", false);\r\n  };\r\n\r\n  const columns = [\r\n    {\r\n      name: \"\",\r\n      selector: (row) => (\r\n        <img src={`${API}/${row.image}`} className=\"w-16 py-3\" />\r\n      ),\r\n    },\r\n    { name: \"الاسم\", selector: (row) => row.name, sortable: true },\r\n    { name: \"السعر\", selector: (row) => row.price, sortable: true },\r\n    {\r\n      name: \"نوع الفئة\",\r\n      selector: (row) => getTypeName(row.type),\r\n      sortable: true,\r\n    },\r\n    {\r\n      name: \"\",\r\n      selector: (row) => (\r\n        <div className=\"gap-x-3 flex \">\r\n          <Link href={`/offers/edit/${row.id}`}>\r\n            <a className=\"bg-primary hover:bg-secondary p-2 rounded-lg text-white\">\r\n              <span className=\"material-icons text-lg\">edit</span>\r\n            </a>\r\n          </Link>\r\n          <ButtonLoader loader={loader.delete}>\r\n            <button\r\n              className=\"bg-red-500 hover:bg-red-400 p-2 rounded-lg text-white\"\r\n              onClick={(_) => handleDelete(row.id)}\r\n            >\r\n              <span className=\"material-icons text-lg\">delete</span>\r\n            </button>\r\n          </ButtonLoader>\r\n        </div>\r\n      ),\r\n      sortable: true,\r\n    },\r\n  ];\r\n\r\n  return (\r\n    <MainPageContainer className=\"\" title=\"العروض\" addLink={\"/offers/add\"}>\r\n      {loader.page ? (\r\n        <div className=\"flex justify-center items-center\">\r\n          <div className=\"material-icons text-6xl text-primary animate-spin\">\r\n            track_changes\r\n          </div>\r\n        </div>\r\n      ) : (\r\n        <>\r\n          <div className=\"w-full p-3 bg-white rounded-lg\">\r\n            <select\r\n              value={activeType}\r\n              className=\"w-full\"\r\n              onChange={(e) => handleChoose(e.target.value)}\r\n            >\r\n              <option value=\"all\">جميع الفئات</option>\r\n              <option value=\"service\">فئات الخدمات</option>\r\n              <option value=\"product\">تسوق اونلاين</option>\r\n            </select>\r\n            <form\r\n              onSubmit={handleSearch}\r\n              className=\"flex space-x-3 space-x-reverse py-3 justify-between\"\r\n            >\r\n              <input\r\n                value={searchData}\r\n                type=\"search\"\r\n                ref={searchRef}\r\n                onChange={(e) => setSearchData(e.target.value)}\r\n                placeholder=\"بحث...\"\r\n                className=\"flex-grow\"\r\n              />\r\n              <button className=\"material-icons\" type=\"submit\">\r\n                search\r\n              </button>\r\n            </form>\r\n          </div>\r\n          {dataItems.length >= 1 ? (\r\n            <>\r\n              <SwitchTransition>\r\n                <CSSTransition\r\n                  key={activeItem.id}\r\n                  classNames=\"fade\"\r\n                  appear={true}\r\n                  timeout={300}\r\n                  unmountOnExit={true}\r\n                  e