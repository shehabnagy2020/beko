lt();\r\n    if (searchData) {\r\n      let item = originalData.filter((i) =>\r\n        i.name.toLowerCase().includes(searchData.toLowerCase())\r\n      );\r\n      setDataItems([...item]);\r\n    } else {\r\n      setDataItems([...originalData]);\r\n      searchRef.current.focus();\r\n    }\r\n  };\r\n\r\n  const handelLoader = (id, val) => {\r\n    setLoader({ ...loader, [id]: val });\r\n  };\r\n\r\n  const getData = async (values = []) => {\r\n    handelLoader(\"page\", true);\r\n    try {\r\n      if (values.length <= 0) {\r\n        const res = await Axios({\r\n          baseURL: API,\r\n          url: \"/api/notifications/getAll\",\r\n          method: \"GET\",\r\n        });\r\n        values = res.data.data;\r\n        console.log(values);\r\n      }\r\n\r\n      if (activeType === \"all\") {\r\n        setOriginalData([...values]);\r\n        setDataItems([...values]);\r\n      } else {\r\n        values = values.filter((i) => i.type === activeType);\r\n        setOriginalData([...values]);\r\n        setDataItems([...values]);\r\n      }\r\n      handelLoader(\"page\", false);\r\n    } catch (error) {}\r\n  };\r\n\r\n  useEffect(() => {\r\n    getData();\r\n  }, [activeType]);\r\n\r\n  const handleChoose = (v) => {\r\n    setActiveType(v);\r\n  };\r\n\r\n  const handleDelete = async (id) => {\r\n    handelLoader(\"delete\", true);\r\n    try {\r\n      const res = await Axios({\r\n        baseURL: API,\r\n        url: \"/api/notification/delete\",\r\n        method: \"DELETE\",\r\n        params: { id },\r\n      });\r\n      await getData([...res.data.data]);\r\n    } catch (error) {\r\n      console.log(error.response);\r\n      alert(\"فشل حذف \");\r\n    }\r\n    handelLoader(\"delete\", false);\r\n  };\r\n\r\n  const columns = [\r\n    { name: \"العنوان\", selector: (row) => row.title, sortable: true },\r\n\r\n    {\r\n      name: \"\",\r\n      selector: (row) => (\r\n        <div className=\"gap-x-3 flex py-3\">\r\n          <Link href={`/notifications/edit/${row.id}`}>\r\n            <a className=\"bg-primary hover:bg-secondary p-2 rounded-lg text-white\">\r\n              <span className=\"material-icons text-lg\">edit</span>\r\n            </a>\r\n          </Link>\r\n          <ButtonLoader loader={loader.delete}>\r\n            <button\r\n              className=\"bg-red-500 hover:bg-red-400 p-2 rounded-lg text-white\"\r\n              onClick={(_) => handleDelete(row.id)}\r\n            >\r\n              <span className=\"material-icons text-lg\">delete</span>\r\n            </button>\r\n          </ButtonLoader>\r\n        </div>\r\n      ),\r\n      sortable: true,\r\n    },\r\n  ];\r\n\r\n  return (\r\n    <MainPageContainer\r\n      className=\"\"\r\n      title=\"الاشعارات\"\r\n      addLink={\"/notifications/add\"}\r\n    >\r\n      {loader.page ? (\r\n        <div className=\"flex justify-center items-center\">\r\n          <div className=\"material-icons text-6xl text-primary animate-spin\">\r\n            track_changes\r\n          </div>\r\n        </div>\r\n      ) : (\r\n        <>\r\n          <div className=\"w-full p-3 bg-white rounded-lg\">\r\n            <select\r\n              value={activeType}\r\n              className=\"w-full\"\r\n              onChange={(e) => handleChoose(e.target.value)}\r\n            >\r\n              <option value=\"all\">جميع الفئات</option>\r\n              <option value=\"service\">فئات الخدمات</option>\r\n              <option value=\"product\">تسوق اونلاين</option>\r\n            </select>\r\n            <form\r\n              onSubmit={handleSearch}\r\n              className=\"flex space-x-3 space-x-reverse py-3 justify-between\"\r\n            >\r\n              <input\r\n                value={searchData}\r\n                type=\"search\"\r\n                ref={searchRef}\r\n                onChange={(e) => setSearchData(e.target.value)}\r\n                placeholder=\"بحث...\"\r\n                className=\"flex-grow\"\r\n              />\r\n